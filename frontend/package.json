{
    "name": "hacklab-booking-frontend",
    "description": "The frontend for the Hacklab booking system",
    "version": "2.0.0",
    "private": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/utmgdsc/hacklab-booking.git"
    },
    "author": "GDSC",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/utmgdsc/hacklab-booking/issues"
    },
    "dependencies": {
        "@emotion/react": "^11.11.0",
        "@emotion/styled": "^11.11.0",
        "@fontsource/roboto": "^5.0.5",
        "@mui/icons-material": "^5.14.0",
        "@mui/material": "^5.14.6",
        "@mui/x-date-pickers-pro": "^6.9.2",
        "axios": "^1.4.0",
        "dayjs": "^1.11.9",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.14.1",
        "react-schedule-selector": "^3.0.2",
        "react-transition-group": "^4.4.5",
        "react-virtuoso": "^4.4.0"
    },
    "devDependencies": {
        "@types/jest": "^29.5.2",
        "@types/node": "^20.4.8",
        "@types/react": "^18.2.8",
        "@types/react-dom": "^18.2.7",
        "@types/react-router-dom": "^5.3.3",
        "prettier": "^3.0.0",
        "react-scripts": "5.0.1",
        "ts-loader": "^9.4.4",
        "typescript": "^4.9.5",
        "typescript-plugin-css-modules": "^5.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "dev": "react-scripts start",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\""
    },
    "prettier": {
        "tabWidth": 4,
        "useTabs": false,
        "singleQuote": true,
        "printWidth": 120
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "parserOptions": {
                    "project": "./tsconfig.json"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
